<p id="notice"><%= notice %></p>

<h1>Api Posts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_posts.each do |api_post| %>
      <tr>
        <td><%= link_to 'Show', api_post %></td>
        <td><%= link_to 'Edit', edit_api_post_path(api_post) %></td>
        <td><%= link_to 'Destroy', api_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Post', new_api_post_path %>
